<apex:page standardController="Account"
	extensions="Nubik_AccountAddContactsToMandateCtrl">
		<apex:form >
			<apex:pageBlock id="theblock" title="Add Contacts to Mandate">
				<apex:pageBlockButtons >
					<apex:commandButton value="Save"
					action="{!CreateApps}" rerender="theblock"/>
					<apex:commandButton value="Back" action="{!back}" />
				</apex:pageBlockButtons>
				<apex:pageMessages />
				<apex:inputField value="{!AppToSelectMandate.Mandate__c}" />
			</apex:pageBlock>
		</apex:form>
		
	
</apex:page>